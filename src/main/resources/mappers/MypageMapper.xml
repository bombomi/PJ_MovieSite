<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MypageMapper">

	<!-- 마이페이지 회원 정보 조회 -->
	<select id="UserAllList" resultType="com.movie.mymovie.dao.MypageDAO">
		SELECT  SEQ, 
	             MEMBER_ID,
	             MEMBER_NICK,
	       		MEMBER_PWD,
	       		MEMBER_EMAIL,
	       		MEMBER_EMAIL2,
	       		MEMBER_PIC     
		FROM    MEMBER
		ORDER BY ID ASC
	</select>
	
</mapper>